<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>User</title>
  </head>
  <body>
    
    <% var id; %>
    <% var users; %>

    <p>USER.EJS</p>
    <% if(id) { %>
    <h4>Details of <%= users[id].name %> </h4>
    <% } else { %>
      USER LIST: 
      <% users.forEach(user => { %>
        <p>- <%=user.name%></p>
      <% }) %>
    <% } %>
  </body>
</html>